  <target name="artifact.aa" depends="init.artifacts, compile.module.aa" description="Build &#39;aa&#39; artifact" xmlns:fx="javafx:com.sun.javafx.tools.ant">
    <mkdir dir="${artifact.output.aa}"/>
    <copy todir="${artifact.temp.output.aa}">
      <fileset dir="${aa.output.dir}"/>
    </copy>
    <fx:fileset id="all_but_aa" dir="${artifact.temp.output.aa}" includes="**/*.jar">
      <exclude name="aa.jar"/>
    </fx:fileset>
    <fx:fileset id="all_aa" dir="${artifact.temp.output.aa}" includes="**/*.jar"/>
    <fx:application id="aa_id" name="aa" mainClass="" version="1.0"/>
    <fx:jar destfile="${artifact.temp.output.aa}/aa.jar">
      <fx:application refid="aa_id"/>
      <fileset dir="${artifact.temp.output.aa}" excludes="**/*.jar"/>
      <fx:resources>
        <fx:fileset refid="all_but_aa"/>
      </fx:resources>
      <manifest>
        <attribute name="Implementation-Title" value="Hello"/>
        <attribute name="Implementation-Version" value="1.0"/>
        <attribute name="Implementation-Vendor" value=""/>
      </manifest>
    </fx:jar>
    <fx:deploy width="600" height="400" updatemode="background" outdir="${artifact.temp.output.aa}/deploy" outfile="aa" nativeBundles="all">
      <fx:application refid="aa_id"/>
      <fx:info title="Hello"/>
      <fx:resources>
        <fx:fileset refid="all_aa"/>
      </fx:resources>
    </fx:deploy>
    <copy todir="${artifact.output.aa}">
      <fileset dir="${artifact.temp.output.aa}/deploy"/>
    </copy>
    <delete includeemptydirs="true">
      <fileset dir="${artifact.temp.output.aa}"/>
    </delete>
  </target>
  
  <target name="build.all.artifacts" depends="artifact.aa" description="Build all artifacts">
    
