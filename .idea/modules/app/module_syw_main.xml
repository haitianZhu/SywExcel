  <patternset id="excluded.from.compilation.syw_main">
    <patternset refid="excluded.from.module.syw_main"/>
  </patternset>
  
  <path id="syw_main.module.sourcepath">
    <dirset dir="${module.syw_main.basedir}/../../../app/src/main">
      <include name="java"/>
      <include name="resources"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.syw_main" depends="compile.module.syw_main.production,compile.module.syw_main.tests" description="Compile module syw_main"/>
  
  <target name="compile.module.syw_main.production" depends="register.custom.compilers" description="Compile module syw_main; production classes">
    <mkdir dir="${syw_main.output.dir}"/>
    <javac2 destdir="${syw_main.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.syw_main}/javac">
      <compilerarg line="${compiler.args.syw_main}"/>
      <bootclasspath refid="syw_main.module.bootclasspath"/>
      <classpath refid="syw_main.module.production.classpath"/>
      <src refid="syw_main.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.syw_main"/>
    </javac2>
    
    <copy todir="${syw_main.output.dir}">
      <fileset dir="${module.syw_main.basedir}/../../../app/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.syw_main.basedir}/../../../app/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.syw_main.tests" depends="register.custom.compilers,compile.module.syw_main.production" description="compile module syw_main; test classes" unless="skip.tests"/>
  
  <target name="clean.module.syw_main" description="cleanup module">
    <delete dir="${syw_main.output.dir}"/>
    <delete dir="${syw_main.testoutput.dir}"/>
  </target>
